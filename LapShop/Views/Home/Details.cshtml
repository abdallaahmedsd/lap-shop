                                        <tr>
                                            <td><strong>@ResAdmin.lblSalesPrice</strong></td>
                                            <td>@Model.VwItem.SalesPrice</td>
                                        </tr>
                                        <tr>
                                            <td><strong>@ResAdmin.lblPurchasePrice</strong></td>
                                            <td>@Model.VwItem.PurchasePrice</td>
